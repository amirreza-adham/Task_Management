{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": { // از localDb در خود vscode استفاده کردیم 
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=TodoDb;Trusted_Connection=True;"
  },
  "JwtSettings": { // اضافه شدن jwt به پروژه 
    "SecretKey": "ThisIsASecretKeyForJwtTokenGeneration12345",
    "Issuer": "TodoApi",
    "Audience": "TodoApiUsers",
    "ExpiryMinutes": 60
  }

}
